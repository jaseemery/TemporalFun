# JFrog Artifactory Credentials
# Copy this file to .env and fill in your actual values
# Default credentials for the local Artifactory instance
ARTIFACTORY_USERNAME=admin
ARTIFACTORY_PASSWORD=password

# Temporal Configuration
TEMPORAL_SERVER=temporal:7233
TASK_QUEUE=default

# Hot Reload Configuration
HOT_RELOAD_ENABLED=true
HOT_RELOAD_MODE=FileSystem

# Artifactory Feed Watching (Alternative to file system watching)
# Set HOT_RELOAD_MODE=ArtifactoryFeed to use feed monitoring instead of file watching
ARTIFACTORY_FEED_URL=http://artifactory:8082/artifactory/api/nuget/v3/nuget
ARTIFACTORY_POLL_INTERVAL_SECONDS=30
ARTIFACTORY_PACKAGE_FILTERS=TemporalActivities,MyWorkflows
ARTIFACTORY_DOWNLOAD_PATH=/tmp/TemporalWorker/FeedPackages

# File System Watching (Default hot reload mode)
HOT_RELOAD_WATCH_PATHS=/home/user/.nuget/packages,/tmp/packages
HOT_RELOAD_FILE_FILTER=*.dll
HOT_RELOAD_DEBOUNCE_MS=1000