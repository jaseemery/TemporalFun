<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="artifactory" value="https://artifactory.example.com/artifactory/api/nuget/v3/nuget" protocolVersion="3">
      <credentials>
        <username>%ARTIFACTORY_USERNAME%</username>
        <password>%ARTIFACTORY_PASSWORD%</password>
      </credentials>
    </add>
  </packageSources>
</configuration>